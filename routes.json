    "preview": {
        "configuration": {
            "sitename": "canvas01-preview.site.pfizer"
        },
        "routes": [
            {
                "sitename": "canvas01-preview.site.pfizer",
                "routepath": "^/$",
                "routes": [
                    {
                        "type": "static",
                        "origin": "https://canvas01-preview.dev.pfizerstatic.io",
                        "requiresAuth": true,
                        "validation": [
                            {
                                "type": "or",
                                "rules": [
                                    {
                                        "type": "grvProfile",
                                        "requires": {
                                            "hcpValidation.status": "notvalidated"
